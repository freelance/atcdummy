<p id="notice"><%= notice %></p>

<h1>Spreadsheets</h1>

<table>
  <thead>
    <tr>
      <th>Number</th>
      <th>Date</th>
      <th>State</th>
      <th>Tneto</th>
      <th>Tgastog</th>
      <th>Tapagar</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @spreadsheets.each do |spreadsheet| %>
      <tr>
        <td><%= spreadsheet.number %></td>
        <td><%= spreadsheet.date %></td>
        <td><%= spreadsheet.state %></td>
        <td><%= spreadsheet.tneto %></td>
        <td><%= spreadsheet.tgastog %></td>
        <td><%= spreadsheet.tapagar %></td>
        <td><%= link_to 'Show', spreadsheet %></td>
        <td><%= link_to 'Edit', edit_spreadsheet_path(spreadsheet) %></td>
        <td><%= link_to 'Destroy', spreadsheet, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Spreadsheet', new_spreadsheet_path %>
