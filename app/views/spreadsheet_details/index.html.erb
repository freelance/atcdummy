<p id="notice"><%= notice %></p>

<h1>Spreadsheet Details</h1>

<table>
  <thead>
    <tr>
      <th>Spreadsheet</th>
      <th>Totalv</th>
      <th>Netov</th>
      <th>Gastov</th>
      <th>Choferv</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @spreadsheet_details.each do |spreadsheet_detail| %>
      <tr>
        <td><%= spreadsheet_detail.spreadsheet_id %></td>
        <td><%= spreadsheet_detail.totalv %></td>
        <td><%= spreadsheet_detail.netov %></td>
        <td><%= spreadsheet_detail.gastov %></td>
        <td><%= spreadsheet_detail.choferv %></td>
        <td><%= link_to 'Show', spreadsheet_detail %></td>
        <td><%= link_to 'Edit', edit_spreadsheet_detail_path(spreadsheet_detail) %></td>
        <td><%= link_to 'Destroy', spreadsheet_detail, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Spreadsheet Detail', new_spreadsheet_detail_path %>
